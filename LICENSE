main()
function main(){
    killAPP("Autox.js v6")
    home();
    exit();
    
}
function killAPP(name){

    var packageName=app.getPackageName(name)

    app.openAppSetting(packageName)

    sleep(500)

    while(true){

       if(text("结束运行"||"强行停止").exists()){

            click("结束运行"||"强行停止");

            sleep(500);

            while(true){

                if(textContains("确定").exists()){

                    !click("确定");

     

                    sleep(1000);

                    break;

                }

                break;

            }

            break;

        }

    }

}
